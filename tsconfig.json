{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@api/*": [
        "./api/*"
      ],
      "@icons/*": [
        "./assets/icons/*"
      ],
      "@img/*": [
        "./assets/images/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@widgets/*": [
        "./widgets/*"
      ],
      "@config/*": [
        "./config/*"
      ],
      "@app/*": [
        "./app/*"
      ],
      "@screens/*": [
        "./screens/*"
      ],
      "@navigators/*": [
        "./navigators/*"
      ],
      "@stores/*": [
        "./stores/*"
      ],
      "@theme/*": [
        "./theme/*"
      ],
      "@hooks/*": [
        "./common/hooks/*"
      ],
      "@interfaces/*": [
        "./common/interfaces/*"
      ],
      "@providers/*": [
        "./common/providers/*"
      ],
      "@constants/*": [
        "./common/constants/*"
      ],
      "@enums/*": [
        "./common/enums/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "global.d.ts", 
    "screens/(notes)/note"  ]
}
